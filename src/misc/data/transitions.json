      "leave": "fadeOutRight"
    },
    "none": {
      "enter": "fadeIn",
      "leave": "fadeOut"
    },
    "right": {
      "enter": "fadeInRight",
      "leave": "fadeOutLeft"
    }
  },
  "fadebig": {
    "name": "Fade (big)",
    "left": {
      "enter": "fadeInLeftBig",
      "leave": "fadeOutRightBig"
    },
    "none": {
      "enter": "fadeIn",
      "leave": "fadeOut"
    },
    "right": {
      "enter": "fadeInRightBig",
      "leave": "fadeOutLeftBig"
    }
  },
  "bounce": {
    "name": "Bounce",
    "left": {
      "enter": "bounceInLeft",
      "leave": "bounceOutRight"
    },
    "none": {
      "enter": "bounceIn",
      "leave": "bounceOut"
    },
    "right": {
      "enter": "bounceInRight",
      "leave": "bounceOutLeft"
    }
  },
  "zoom": {
    "name": "Zoom",
    "left": {
      "enter": "zoomInLeft",
      "leave": "zoomOutRight"
