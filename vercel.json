{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=7776000"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=7776000"
        }
      ]
    }
  ]
}
